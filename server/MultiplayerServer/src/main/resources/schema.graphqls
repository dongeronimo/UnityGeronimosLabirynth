type Greeting {
    id: ID!
    message: String!
}

type Vector3 {
    x: Float!
    y: Float!
    z: Float!
}
type GameObject {
    id: String!
    position: Vector3!
    parent: GameObject
    children: [GameObject]!
}

type Query {
    getGreeting(id: ID!):Greeting
    getGameObjectById(id: String!):GameObject
}

type Mutation {
    newGreeting(message:String!):Greeting!
    newGameObject(positionX:Float!, positionY:Float!, positionZ: Float!):GameObject!
}