type Vector3 {
    x: Float!
    y: Float!
    z: Float!
}
type GameObject {
    id: String!
    position: Vector3!
    parent: GameObject
    children: [GameObject]!
}

input PositionInput {
    x: Float!
    y: Float!
    z: Float!
}
type Query {
    getGameObjectById(id: String!):GameObject
}

type Mutation {
    newGameObject(position:PositionInput):GameObject!
    # newGameObject(positionX:Float!, positionY:Float!, positionZ: Float!):GameObject!
}