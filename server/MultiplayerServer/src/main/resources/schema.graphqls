type Greeting {
    id: ID!
    message: String!
}

type GameObject {
    id: String!
    positionX : Float!
    positionY : Float!
    positionZ : Float!
    parent: GameObject
    children: [GameObject]
}

type Query {
    getGreeting(id: ID!):Greeting
}

type Mutation {
    newGreeting(message:String!):Greeting!
}